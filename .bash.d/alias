alias rawls='/bin/ls'
alias ls='ls --color=auto --show-control-char'
alias l='ls -l'
alias la='ls -la'
alias ee='exit'
alias e='exit'
alias rm='rm -i'
alias lr='less -r'
alias grep='grep --color=auto -i -P'
alias cgrep='/bin/grep --color=always'
alias ngrep='/bin/grep --color=never'

alias c='cd'
alias x='cd ..'
alias b='cd -'
alias v='vim'
alias view='vim -R'
alias g='newgrp'
alias i='id -a'
